    <recordCreates>
        <name>NewCallLog</name>
        <label>NewCallLog</label>
        <locationX>1077</locationX>
        <locationY>126</locationY>
        <connector>
            <targetReference>End</targetReference>
        </connector>
        <inputAssignments>
            <field>CallDurationInSeconds</field>
