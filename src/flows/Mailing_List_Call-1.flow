                <elementReference>$Flow.CurrentDateTime</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>NewCallLog</targetReference>
        </connector>
    </assignments>
    <formulas>
        <name>Duration</name>
        <dataType>Number</dataType>
        <expression>({!EndTime}-{!Starttime})*100000</expression>
        <scale>8</scale>
    </formulas>
    <interviewLabel>Mailing List Call {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Mailing List Call</label>
    <processType>Flow</processType>
