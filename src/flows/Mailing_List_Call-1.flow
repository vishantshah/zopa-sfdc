<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <assignments>
        <name>AssignStartTime</name>
        <label>AssignStartTime</label>
        <locationX>133</locationX>
        <locationY>121</locationY>
        <assignmentItems>
            <assignToReference>Starttime</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>$Flow.CurrentDateTime</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Intro</targetReference>
        </connector>
    </assignments>
    <assignments>
        <name>SetEndTime</name>
        <label>SetEndTime</label>
        <locationX>924</locationX>
        <locationY>124</locationY>
        <assignmentItems>
